---
const { id } = Astro.props;
---

<video
    loop
    autoplay="true"
    muted
    class="border-black border-2 rounded"
    preload="none"
>
 <source
        src={`/vids/webm/${id}.webm`}
        type={`video/webm;`}
    />
    <!-- <source
        src={`/vids/mp4/${id}.mp4`}
        type={`video/mp4;`}
    /> -->
</video>
